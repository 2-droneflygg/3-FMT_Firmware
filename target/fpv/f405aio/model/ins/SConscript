from building import *

cwd = GetCurrentDir()
src	= []
inc = []

src += Glob('*.c')
#src += Glob('../lib/*.c')
src += Glob('../../../../../../src/model/ins/base_ins/lib/*.c')


inc += [cwd]
inc += [cwd + '../../../../../../src/model/ins/base_ins/lib']

group = DefineGroup('Base_INS', src, depend = [''], CPPPATH = inc)

Return('group')
