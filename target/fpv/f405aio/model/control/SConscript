from building import *

cwd = GetCurrentDir()
src	= []
inc = []

src += Glob('*.c')
#src += Glob('lib/*.c')
src += Glob('../../../../../src/model/control/base_controller/lib/*.c')

inc += [cwd]
inc += [cwd + '../../../../../src/model/control/base_controller/lib']

group = DefineGroup('Base_Control', src, depend = [''], CPPPATH = inc)

Return('group')
