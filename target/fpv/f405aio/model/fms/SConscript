from building import *

cwd = GetCurrentDir()
src	= []
inc = []

src += Glob('*.c')
#src += Glob('lib/*.c')
src += Glob('../../../../../../src/model/fms/base_fms/lib/*.c')

inc += [cwd]
inc += [cwd + '../../../../../../src/model/fms/base_fms/lib']

group = DefineGroup('Base_FMS', src, depend = [''], CPPPATH = inc)

Return('group')
